{
  "key": "",
  "manifest_version": 3,
  "name": "Paid Tweets - X to Earn",
  "description": "Turn your X tweets into valuable datasets for model training and earn cryptocurrency rewards.",
  "version": "0.1.1",
  "icons": {
    "16": "logo-daeta.png",
    "32": "logo-daeta.png",
    "48": "logo-daeta.png",
    "64": "logo-daeta.png",
    "128": "logo-daeta.png"
  },
  "action": {
    "default_icon": {
      "16": "logo-daeta.png",
      "32": "logo-daeta.png",
      "48": "logo-daeta.png",
      "64": "logo-daeta.png",
      "128": "logo-daeta.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "background", "activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "js": ["src/content.ts"],
      "matches": ["https://x.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://x.com/*"],
      "resources": ["*"]
    }
  ],
  "minimum_chrome_version": "102",
  "host_permissions": ["<all_urls>"],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  }
}
